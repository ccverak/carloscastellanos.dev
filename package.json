{
  "name": "carloscastellanos.dev",
  "version": "2.0.0",
  "scripts": {
    "build": "npm-run-all clean build:assets build:site purge",
    "build:assets": "NODE_ENV=production webpack --mode=production",
    "build:site": "NODE_ENV=production eleventy",
    "clean": "rm -rf ./dist",
    "dev": "npm-run-all clean webpack:assets --parallel dev:*",
    "dev:assets": "yarn run webpack:assets --watch",
    "dev:site": "NODE_ENV=development eleventy --serve",
    "webpack:assets": "NODE_ENV=development webpack --mode=development",
    "purge": "purgecss --config purgecss.config.js --output dist/assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccverak/carloscastellanos.dev"
  },
  "author": {
    "name": "Carlos Castellanos"
  },
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "homepage": "https://github.com/ccverak/carloscastellanos.dev",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.5",
